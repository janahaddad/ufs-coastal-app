          "properties": {
            "dtlimit": {
              "type": "number"
            },
            "mapalgo": {
              "type": "string"
            },
            "readmode": {
              "enum": [
                "single"
              ]
            },
            "stream_data_files": {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            "stream_data_variables": {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            "data": {
              "additionalProperties": false,
              "properties": {
                "source": {
                  "enum": [
                    "hrrr",
                    "gfs"
                  ]
                },
                "length": {
                  "type": "number"
                },
                "fxx": {
                  "type": "number"
                },
                "subset": {
                  "type": "boolean"
                },
                "overwrite": {
                  "type": "boolean"
                },
                "target_directory": {
                  "type": "string"
                },
                "protocol": {
                  "enum": [
                    "herbie",
                    "wget",
                    "s3"
                  ]
                },
                "end_point": {
                  "type": "string"
                },
                "files": {
