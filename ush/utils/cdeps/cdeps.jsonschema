              }
            },
            "stream_lev_dimname": {
              "type": "string"
            },
            "stream_mesh_file": {
              "type": "string"
            },
            "stream_offset": {
              "type": "integer"
            },
            "stream_vectors": {
              "items": {
                "type": "string"
              },
              "type": [
                "array",
                "string"
              ]
            },
            "taxmode": {
              "type": "string"
            },
            "tinterpalgo": {
              "type": "string"
            },
            "yearAlign": {
              "type": "integer"
            },
            "yearFirst": {
              "type": "integer"
            },
            "yearLast": {
              "type": "integer"
            }
          },
          "required": [
            "stream_data_files",
            "stream_data_variables",
            "stream_mesh_file"
          ],
          "type": "object"
        }
      },
      "type": "object"
    }
  },
  "type": "object"
